<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN">
  <appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [Projet 3] %-5level %logger{36} - %msg%n"/>
    </Console>
    <File name="File" fileName="logs/projet.log" immediateFlush="true">
        <PatternLayout pattern="%d{HH:mm:ss.SSS} [Projet 3] %-5level %logger{36} - %msg%n"/>
    </File>
  </appenders>
  <loggers>
    <root level="debug">
      <appender-ref ref="Console"/>
    </root>
  </loggers>
</configuration>